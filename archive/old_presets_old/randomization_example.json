  "api": {
    "base_url": "http://127.0.0.1:7860",
    "timeout": 300
  },
  "randomization": {
    "enabled": true,
    "prompt_sr": {
      "enabled": true,
      "mode": "random",
      "rules": [
        {
          "search": "person",
          "replacements": ["man", "woman", "child", "elder"]
        },
        {
          "search": "pose",
          "replacements": ["standing", "sitting", "walking", "running"]
        }
      ],
      "raw_text": "person => man | woman | child | elder\npose => standing | sitting | walking | running"
    },
    "wildcards": {
      "enabled": true,
      "mode": "random",
      "tokens": [
        {
          "token": "__mood__",
          "values": ["happy", "sad", "angry", "contemplative", "serene"]
        },
        {
          "token": "__weather__",
          "values": ["sunny", "cloudy", "rainy", "foggy", "stormy"]
        },
        {
          "token": "__lighting__",
          "values": ["golden hour", "blue hour", "midday sun", "moonlight", "dramatic lighting"]
        }
      ],
      "raw_text": "__mood__: happy | sad | angry | contemplative | serene\n__weather__: sunny | cloudy | rainy | foggy | stormy\n__lighting__: golden hour | blue hour | midday sun | moonlight | dramatic lighting"
    },
    "matrix": {
      "enabled": true,
      "mode": "fanout",
      "limit": 8,
      "slots": [
        {
          "name": "time",
          "values": ["dawn", "noon", "dusk", "midnight"]
        },
        {
          "name": "location",
          "values": ["forest", "beach", "mountain", "city"]
        }
      ],
      "raw_text": "time: dawn | noon | dusk | midnight\nlocation: forest | beach | mountain | city"
    }
  },
  "aesthetic": {
    "enabled": false,
    "mode": "script",
    "weight": 0.9,
    "steps": 5,
    "learning_rate": 0.0001,
    "slerp": false,
    "slerp_angle": 0.1,
    "embedding": "None",
    "text": "",
    "text_is_negative": false,
    "fallback_prompt": ""
  },
  "pipeline": {
    "txt2img_enabled": true,
    "img2img_enabled": false,
    "adetailer_enabled": true,
    "upscale_enabled": true,
    "video_enabled": false,
    "loop_type": "single",
    "loop_count": 1,
    "pack_mode": "selected",
    "images_per_prompt": 1,
    "model_matrix": [],
